{
  "name": "bits-to-dead-trees",
  "version": "1.0.0",
  "private": true,
  "description": "Turn HTML into PDF files",
  "main": "dist/index.js",
  "devDependencies": {
    "@types/node": "^16",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "typescript": "^4.6.2"
  },
  "scripts": {
    "lint": "eslint src/*.ts",
    "format": "prettier --write src/*.ts",
    "build": "tsc -p tsconfig.json --preserveWatchOutput",
    "start": "node dist/index.js",
    "start:dev": "concurrently --kill-others \"npm run build -- --watch\" \"nodemon --inspect dist/index.js\" "
  },
  "engines": {
    "node": ">= 16"
  },
  "keywords": [],
  "author": "Robin Mehner <robin@coding-robin.de>",
  "license": "Apache-2.0",
  "type": "module",
  "dependencies": {
    "fastify": "^3.27.4"
  }
}
